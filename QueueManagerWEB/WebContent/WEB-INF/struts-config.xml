<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<!-- Data Sources -->
	<data-sources>
	</data-sources>
 
	<!-- Form Beans -->
	<form-beans>
		<form-bean name="testAddQueue" type="org.apache.struts.action.DynaActionForm">
			<form-property name="queueName" type="java.lang.String">
			</form-property>
			<form-property name="size" type="java.math.BigDecimal">
			</form-property>
			<form-property name="isterminate" type="java.lang.String">
			</form-property>
			<form-property name="workingtime" type="java.math.BigDecimal">
			</form-property>
			<form-property name="activationclass" type="java.lang.String">
			</form-property>
			<form-property name="actiontype" type="java.lang.String" initial="N">
			</form-property>
		</form-bean>
		<form-bean name="testputform" type="org.apache.struts.action.DynaActionForm">
			<form-property name="queueName" type="java.lang.String">
			</form-property>
			<form-property name="message" type="java.lang.String">
			</form-property>
			<form-property name="action" initial="N" type="java.lang.String">
			</form-property>
			<form-property name="piority" type="java.lang.Integer">
			</form-property>
			<form-property name="status" initial="R" type="java.lang.String">
			</form-property>
			<form-property name="jobgroup" type="java.lang.Long">
			</form-property>
			<form-property name="messageFrom" type="java.lang.String">
			</form-property>
			<form-property name="messageTo" type="java.lang.String">
			</form-property>
			<form-property name="scrapType" type="java.lang.String" initial="one">
			</form-property>
		</form-bean>
		<form-bean name="testarrayform" type="org.apache.struts.action.DynaActionForm">
			<form-property name="set" type="java.lang.String[]">
			</form-property>
			<form-property name="action" type="java.lang.String" initial="a">
			</form-property>
		</form-bean>
		<form-bean name="queueConsoleForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="switchs" type="java.lang.String">
			</form-property>
			<form-property name="size" type="java.lang.Integer">
			</form-property>
			<form-property name="availabelThread" type="java.lang.Integer">
			</form-property>
			<form-property name="releaseJob" type="java.lang.Integer">
			</form-property>
			<form-property name="holdJob" type="java.lang.Integer">
			</form-property>
			<form-property name="processingJob" type="java.lang.Integer">
			</form-property>
			<form-property name="queueName" type="java.lang.String">
			</form-property>
			<form-property name="allQueueName" type="java.util.Collection">
			</form-property>
			<form-property name="action" type="java.lang.String" initial="init">
			</form-property>
		</form-bean>
		<form-bean name="scrapJobViewerForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="jobGroup" type="java.lang.String">
			</form-property>
			<form-property name="submitUser" type="java.lang.String">
			</form-property>
			<form-property name="submitTimeFrom" type="java.lang.String">
			</form-property>
			<form-property name="status" type="java.lang.String[]">
			</form-property>
			<form-property name="action" initial="init" type="java.lang.String">
			</form-property>
			<form-property name="submitTimeTo" type="java.lang.String">
			</form-property>
			<form-property name="referenceNo" type="java.lang.String">
			</form-property>
			<form-property name="qname" type="java.lang.String">
			</form-property>
			<form-property name="jobidList" type="java.lang.String[]">
			</form-property>
		</form-bean>
	</form-beans>

	<!-- Global Exceptions -->
	<global-exceptions>
	</global-exceptions>

	<!-- Global Forwards -->
	<global-forwards>
		<forward name="error" path="error.jsp">
		</forward>
	</global-forwards>

	<!-- Action Mappings -->
	<action-mappings>
		<action path="/testAddQueue" type="th.co.testqueuemanagerweb.TestAddQueueAction" name="testAddQueue" scope="request" attribute="testAddQueue">
			<forward name="success" path="index.jsp">
			</forward>
		</action>
		<action path="/testPutjob" type="th.co.testqueuemanagerweb.TestPutQueueAction" name="testputform" scope="request" attribute="testputform">
			<forward name="success" path="putjob.jsp">
			</forward>
		</action>
		<action path="/testArray" type="th.co.testqueuemanagerweb.TestArrayAction" name="testarrayform" scope="request" attribute="testarrayform">
			<forward name="success" path="teststrutsarray.jsp">
			</forward>
		</action>
		<action path="/queueConsoleAction" type="th.co.testqueuemanagerweb.QueueConsoleAction" name="queueConsoleForm" scope="request" attribute="queueConsoleForm">
			<forward name="success" path="queueConsole.jsp">
			</forward>
		</action>
		<action path="/scrapJobViewer" type="th.co.testqueuemanagerweb.ScrapJobViewerAction" name="scrapJobViewerForm" scope="request" attribute="scrapJobViewerForm">
			<forward name="success" path="scrapJobViewer.jsp">
			</forward>
		</action>
	</action-mappings>

	<!-- Message Resources -->
	<message-resources parameter="th.co.testqueuemanagerweb.resources.ApplicationResources"></message-resources>

</struts-config>
